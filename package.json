{
  "name": "bus-api",
  "version": "1.0.0",
  "description": "This project contains source code and supporting files for a serverless application that you can deploy with the SAM CLI. It includes the following files and folders.",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.917.0",
    "@aws-sdk/lib-dynamodb": "^3.917.0"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "jest": "^30.2.0"
  },
  "jest": {
    "testEnvironment": "node",
    "testMatch": ["<rootDir>/hello-world/tests/unit/**/*.test.js"],
    "clearMocks": true,
    "testPathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/.aws-sam/"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/.aws-sam/"
    ]
  }
}
